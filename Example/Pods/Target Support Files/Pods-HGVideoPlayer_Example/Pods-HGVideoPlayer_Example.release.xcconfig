FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/../../HGVideoPlayer/Classes/framework/vod"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/HGVideoPlayer" "${PODS_ROOT}/Headers/Public/Masonry"
LIBRARY_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/HGVideoPlayer" "$PODS_CONFIGURATION_BUILD_DIR/Masonry"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/HGVideoPlayer" -isystem "${PODS_ROOT}/Headers/Public/Masonry"
OTHER_LDFLAGS = $(inherited) -ObjC -l"HGVideoPlayer" -l"Masonry" -l"bz2" -l"iconv" -l"stdc++" -l"stdc++.6" -l"z" -framework "Foundation" -framework "KSYMediaPlayer" -framework "UIKit" -framework "VideoToolbox"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
